CREATE DATABASES NATIONALPARKS;

- -PARKS TABLE--

CREATE TABLE Parks (
    id INTEGER PRIMARY KEY,
    name VARCHAR(255),
    details VARCHAR(255),
    timing TIME,
    fee VARCHAR(255),
    vehicle_options VARCHAR(255)
);

